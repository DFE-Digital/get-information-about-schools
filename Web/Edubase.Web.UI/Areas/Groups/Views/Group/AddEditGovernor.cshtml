@using VM = Edubase.Web.UI.Areas.Groups.Models.CreateEdit.GroupEditGovernanceViewModel;

@model VM
@{
    ViewBag.bodyClasses = "create-edit-school school-details";
    ViewBag.hideLogo = true;
    //ViewBag.Title = Model.PageTitle;
    ViewBag.SelectedTab = "EditGovernance";
    Layout = "_EditLayoutPage.cshtml";

    if (Request.HttpMethod == "GET")
    {
        Html.RenderAction("AddEdit", "Governor", new { area = "Governors", groupUId = Model.GroupUId });
    }
    else if (Request.HttpMethod == "POST")
    {
        Html.RenderAction("AddEditPost", "Governor", new { area = "Governors" });
    }
}
