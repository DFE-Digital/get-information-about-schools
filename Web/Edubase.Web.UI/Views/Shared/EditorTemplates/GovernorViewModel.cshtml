@using Edubase.Common
@using Edubase.Services.Enums
@model Edubase.Web.UI.Areas.Governors.Models.GovernorViewModel

@if (EnumSets.eGovernanceProfessionalRoles.Contains(Model.GovernorRole))
{
    @Html.Partial("_GovernorEditGid")
    @Html.Partial("_GovernorEditName")
    @Html.Partial("_GovernorEditDateOfBirth")
    @Html.Partial("_GovernorEditPostcode")
    @Html.Partial("_GovernorEditTelephoneNumber")
    @Html.Partial("_GovernorEditEmail")
    @Html.Partial("_GovernorEditAppointmentDates")
}
else
{
    @Html.Partial("_GovernorEditName")
    @Html.Partial("_GovernorEditGid")
    @Html.Partial("_GovernorEditAppointingBody")
    @Html.Partial("_GovernorEditAppointmentDates")
    @Html.Partial("_GovernorEditEmail")
    @Html.Partial("_GovernorEditPostcode")
    @Html.Partial("_GovernorEditDateOfBirth")
    @Html.Partial("_GovernorEditPreviousName")
    @Html.Partial("_GovernorEditTelephoneNumber")
}

